<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- DataStoreファイルをクラウドバックアップ対象に含める -->
        <include domain="file" path="datastore/" />
    </cloud-backup>
    <device-transfer>
        <!-- デバイス間転送時もデータを含める -->
        <include domain="file" path="datastore/" />
    </device-transfer>
</data-extraction-rules>
